---
import { getCollection } from 'astro:content';

const allServicesRaw = await getCollection('services');
console.log('=== RAW SERVICES ===');
console.log('Total services entries:', allServicesRaw.length);
allServicesRaw.forEach((s, i) => {
  console.log(`[${i}] ID: ${s.id}, Data type: ${typeof s.data}, Is Array: ${Array.isArray(s.data)}`);
  if (Array.isArray(s.data)) {
    console.log(`  Array length: ${s.data.length}`);
  }
});

const localized = await Astro.locals.content.getLocalizedCollection('services');
console.log('\n=== LOCALIZED SERVICES ===');
console.log('Total localized:', localized.length);
localized.forEach((s, i) => {
  console.log(`[${i}] ID: ${s.id}, Data: ${JSON.stringify(s.data).substring(0, 100)}`);
});
