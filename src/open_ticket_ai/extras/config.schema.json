              "type": "object"
            },
            {
              "type": "string"
            }
          ],
          "title": "Injects"
        },
        "if": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": "True",
          "title": "If"
        },
        "depends_on": {
          "anyOf": [
            {
              "type": "string"
