{
  "$defs": {
    "InfrastructureConfig": {
      "additionalProperties": true,
      "properties": {
        "logging": {
          "$ref": "#/$defs/LoggingConfig"
        },
        "default_template_renderer": {
          "title": "Default Template Renderer",
          "type": "string"
        }
      },
      "required": [
        "default_template_renderer"
      ],
      "title": "InfrastructureConfig",
      "type": "object"
    },
    "LoggingConfig": {
      "properties": {
        "level": {
          "default": "INFO",
          "enum": [
            "DEBUG",
            "INFO",
            "WARNING",
            "ERROR",
            "CRITICAL"
          ],
          "title": "Level",
          "type": "string"
        },
        "log_to_file": {
          "default": false,
          "title": "Log To File",
          "type": "boolean"
        },
        "log_file_path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Log File Path"
        },
        "log_format": {
          "default": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
          "title": "Log Format",
          "type": "string"
        },
        "date_format": {
          "default": "%Y-%m-%d %H:%M:%S",
          "title": "Date Format",
          "type": "string"
        }
      },
      "title": "LoggingConfig",
      "type": "object"
    },
    "OrchestratorConfig": {
      "additionalProperties": true,
      "properties": {
        "runners": {
          "items": {
            "$ref": "#/$defs/RunnerDefinition"
          },
          "title": "Runners",
          "type": "array"
        }
      },
      "title": "OrchestratorConfig",
      "type": "object"
    },
    "PipeConfig": {
      "additionalProperties": false,
      "properties": {
        "uid": {
          "title": "Uid",
          "type": "string"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "use": {
          "default": "open_ticket_ai.base.CompositePipe",
          "title": "Use",
          "type": "string"
        },
        "injects": {
          "additionalProperties": {
            "type": "string"
          },
          "title": "Injects",
          "type": "object"
        },
        "params": {
          "additionalProperties": true,
          "title": "Params",
          "type": "object"
        },
        "if": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": "True",
          "title": "If"
        },
        "depends_on": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          ],
          "default": [],
          "title": "Depends On"
        },
        "steps": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/PipeConfig"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Steps"
        }
      },
      "required": [
        "id"
      ],
      "title": "PipeConfig",
      "type": "object"
    },
    "RenderableConfig": {
      "additionalProperties": false,
      "properties": {
        "uid": {
          "title": "Uid",
          "type": "string"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "use": {
          "default": "open_ticket_ai.base.CompositePipe",
          "title": "Use",
          "type": "string"
        },
        "injects": {
          "additionalProperties": {
            "type": "string"
          },
          "title": "Injects",
          "type": "object"
        },
        "params": {
          "additionalProperties": true,
          "title": "Params",
          "type": "object"
        }
      },
      "required": [
        "id"
      ],
      "title": "RenderableConfig",
      "type": "object"
    },
    "RunnerDefinition": {
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Id"
        },
        "on": {
          "items": {
            "$ref": "#/$defs/TriggerConfig"
          },
          "title": "On",
          "type": "array"
        },
        "run": {
          "$ref": "#/$defs/PipeConfig"
        }
      },
      "required": [
        "on",
        "run"
      ],
      "title": "RunnerDefinition",
      "type": "object"
    },
    "TriggerConfig": {
      "additionalProperties": false,
      "properties": {
        "uid": {
          "title": "Uid",
          "type": "string"
        },
        "id": {
          "title": "Id",
          "type": "string"
        },
        "use": {
          "default": "open_ticket_ai.base.CompositePipe",
          "title": "Use",
          "type": "string"
        },
        "injects": {
          "additionalProperties": {
            "type": "string"
          },
          "title": "Injects",
          "type": "object"
        },
        "params": {
          "additionalProperties": true,
          "title": "Params",
          "type": "object"
        }
      },
      "required": [
        "id"
      ],
      "title": "TriggerConfig",
      "type": "object"
    }
  },
  "additionalProperties": true,
  "properties": {
    "plugins": {
      "items": {
        "type": "string"
      },
      "title": "Plugins",
      "type": "array"
    },
    "infrastructure": {
      "$ref": "#/$defs/InfrastructureConfig"
    },
    "services": {
      "items": {
        "$ref": "#/$defs/RenderableConfig"
      },
      "title": "Services",
      "type": "array"
    },
    "orchestrator": {
      "$ref": "#/$defs/OrchestratorConfig"
    }
  },
  "title": "RawOpenTicketAIConfig",
  "type": "object"
}