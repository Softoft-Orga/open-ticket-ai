            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Filters"
        }
      },
      "title": "LoggerConfig",
      "type": "object"
    },
    "LoggingDictConfig": {
      "additionalProperties": true,
      "properties": {
        "version": {
          "const": 1,
          "default": 1,
          "title": "Version",
          "type": "integer"
        },
        "disable_existing_loggers": {
          "anyOf": [
            {
              "type": "boolean"
