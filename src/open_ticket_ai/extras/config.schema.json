    "TemplateRendererEnvConfig": {
      "properties": {
        "prefix": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "OTAI_",
          "description": "Primary environment variable prefix",
          "title": "Prefix"
        },
        "allowlist": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array",
              "uniqueItems": true
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Allowed environment variable names",
          "title": "Allowlist"
        },
        "denylist": {
          "anyOf": [
            {
              "items": {
                "type": "string"
