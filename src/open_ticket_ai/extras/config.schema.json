        "loggers": {
          "additionalProperties": {
            "$ref": "#/$defs/LoggerConfig"
          },
          "title": "Loggers",
          "type": "object"
        },
        "handlers": {
          "additionalProperties": {
            "$ref": "#/$defs/HandlerConfig"
          },
          "title": "Handlers",
          "type": "object"
        },
        "formatters": {
          "additionalProperties": {
            "$ref": "#/$defs/FormatterConfig"
          },
          "title": "Formatters",
          "type": "object"
        },
        "filters": {
          "additionalProperties": {
            "$ref": "#/$defs/FilterConfig"
          },
          "title": "Filters",
          "type": "object"
        }
      },
      "title": "LoggingDictConfig",
      "type": "object"
    },
    "MustacheRendererConfig": {
      "additionalProperties": true,
