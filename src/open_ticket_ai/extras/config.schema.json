    "RunnerDefinition": {
      "properties": {
        "run_every_milli_seconds": {
          "minimum": 1,
          "title": "Run Every Milli Seconds",
          "type": "integer"
        },
        "pipe": {
          "$ref": "#/$defs/RawPipeConfig"
        }
