    "GeneralConfig": {
      "additionalProperties": true,
      "properties": {
        "logging": {
          "$ref": "#/$defs/LoggingDictConfig"
        },
        "template_renderer": {
          "discriminator": {
            "mapping": {
              "jinja": "#/$defs/JinjaRendererConfig",
              "mustache": "#/$defs/MustacheRendererConfig"
            },
            "propertyName": "type"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/JinjaRendererConfig"
            },
            {
              "$ref": "#/$defs/MustacheRendererConfig"
            }
          ],
          "title": "Template Renderer"
        }
      },
      "title": "GeneralConfig",
      "type": "object"
    },
    "HandlerConfig": {
      "additionalProperties": true,
      "properties": {
        "class": {
          "title": "Class",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "enum": [
                "NOTSET",
                "DEBUG",
                "INFO",
                "WARNING",
                "ERROR",
                "CRITICAL"
              ],
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Level"
        },
        "formatter": {
          "anyOf": [
            {
              "type": "string"
            },
